%{

  open Abstract_constraint;;

%}
%start constraint_entry
%start expr
%token VERTICALBAR
%token TRUE
%token SEMICOLON
%token PARGAUCHE
%token PARDROITE
%token <Numbers.t> INT
%token <string> IDENT
%token FALSE
