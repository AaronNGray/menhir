%start main
%token B
%token A
%nonassoc block_is_finished 
%nonassoc A 
%type <unit> main
%%

main:
_1 = list_block_ _2 = B
    {                  ()}

block:
_1 = items
    {                  ()}

items:
_1 = item %prec block_is_finished
    {                  ()}
| _1 = item _2 = items
    {                  ()}

item:
_1 = A
    {                  ()}

list_block_:

    {    ( [] )}
| x = block xs = list_block_
    {    ( x :: xs )}

%%



