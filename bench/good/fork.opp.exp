%token KIND
%token <string> INCLUDE
%token IN
%token <string * Lexing.position * Lexing.position> IDENTIFIER
%token FORALL
%token EXISTS
%token EQ
%token EOF
%token DOT
%token DEFAULT
%token COMMA
%token COLON
%token BIGLAMBDA
%token BACKQUOTE
%token ASSERT
%token AS
%token ARROW

%type <Syntax.declarations> program
